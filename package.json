{
  "name": "droplater",
  "version": "1.0.0",
  "description": "A service where users create notes that should be sent to a webhook at/after a time",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:worker\" \"npm run dev:sink\"",
    "dev:api": "nodemon api/server.js",
    "dev:worker": "nodemon worker/index.js",
    "dev:sink": "nodemon sink/server.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "seed": "node scripts/seed.js",
    "build-admin": "node scripts/build-admin.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "bullmq": "^5.0.0",
    "redis": "^4.6.10",
    "zod": "^3.22.4",
    "pino": "^8.17.2",
    "pino-pretty": "^10.2.3",
    "dayjs": "^1.11.10",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "keywords": ["webhook", "scheduler", "notes", "delivery"],
  "author": "",
  "license": "MIT"
}
